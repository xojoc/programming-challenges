   ],
   "source": [
    "from math import gcd\n",
    "\n",
    "def lcm(a, b):\n",
    "    return a*b // gcd(a, b)\n",
    "\n",
    "l = 1\n",
    "for n in range(1,20+1):\n",
    "    l = lcm(l, n)\n",
    "l"
   ]
